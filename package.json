{
  "name": "mongo",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "rimraf dist&&babel src -d dist --source-maps --copy-files",
    "serve": "set NODE_ENV=production&& node --require @babel/register --require @babel/polyfill dist/app.js",
    "dev": "set NODE_ENV=development&& nodemon src/app.js --exec babel-node ",
    "test:mocha": "set NODE_ENV=test&& mocha --require @babel/register --require @babel/polyfill --reporter spec --exit tests/mocha --exec babel-node",
    "test:unit": "set NODE_ENV=unit&& babel-node spec/unit.js",
    "test:integration": "set NODE_ENV=integration&& babel-node spec/integration.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.3.0",
    "@line/bot-sdk": "^6.8.2",
    "body-parser": "^1.19.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "log4js": "^5.1.0",
    "luxon": "^1.19.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongodb": "^3.3.0",
    "mongoose": "^5.6.9",
    "morgan": "^1.9.1",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.6.0",
    "chai": "^4.2.0",
    "jasmine": "^3.4.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
